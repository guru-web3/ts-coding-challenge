{
  "name": "tha-coding-challenge",
  "version": "0.0.1",
  "description": "basic Typescript project with Gherkin using HCS and HTS",
  "main": "index.js",
  "repository": "git@github.com:drgorb/tha-coding-challenge.git",
  "author": "Micha Roon <micha.roon@managination.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "cucumber-js -p default --exit",
    "test:dev": "cucumber-js -p default --tags '@dev' --exit",
    "test:wip": "cucumber-js -p default --tags '@wip' --exit",
    "lint": "eslint . --ext .ts,.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@eslint/js": "^9.21.0",
    "@hashgraph/sdk": "^2.47.0",
    "@types/cucumber": "^7.0.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "cucumber-pretty": "^6.0.1",
    "cucumber-tsflow": "^4.4.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "mocha": "^10.5.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.5.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.25.0"
  }
}
